	<e><p><l>آپڑیاں<s n="p3"/><s n="m"/></l><r>اپنے<s n="p3"/><s n="m"/></r></p></e>
	<e><p><l>اُنھاں<s n="p3"/><s n="m"/></l><r>انہوں<s n="p3"/><s n="m"/></r></p></e>
	<e><p><l>اون٘دا<s n="p3"/><s n="m"/></l><r>اس<s n="p3"/><s n="m"/></r></p></e>
	
	<!-- SECTION: Adjectives -->
	<e><p><l>بد<s n="n"/><s n="m"/></l><r>بد<s n="n"/><s n="m"/></r></p></e>
	



	<!-- SECTION: Adverbs -->
	
	<e><p><l>وِی<s n="adv"/></l><r>بھی<s n="adv"/></r></p></e>

